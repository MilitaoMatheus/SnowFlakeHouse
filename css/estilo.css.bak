	position:absolute;
}

.slider li {
	list-style: none; /* tipo de lista nenhum */
	position: absolute; /* posição da imagem absoluta */
	border-radius:10px;
}

.slider img {
	height: 300px;
	width:670px;
	border-radius:10px;
}

.slider input {
	display: none;
}

.slider {
	height: 300px;
	width: 100%;
	position: relative;
}

.slider label {
	border-radius:5px;
	background-color: #98f0f1;
	bottom: 10px;
	cursor: pointer;
	display: block;
	height: 20px;
	position: absolute;
	width: 20px;
	z-index: 10; /*O eixo Z é o eixo responsável pelo cálculo e posicionamento da profundidade de algum
elemento */
}

.slider img {
	opacity: 0;
	visibility: hidden; /*visibilidade em hiden oculta */
}

.slider li input:checked ~ img {
	opacity: 1;
	visibility: visible;
	z-index: 10; }
	
.slider li:nth-child(1) label { /* nth-child chama um conjunto de elementos filhos ou seja inicia as imagens */
	left: 10px; /* posição da distancia da imagem uma da outra*/
}

.slider li:nth-child(2) label {
	left: 40px;
}

.slider li:nth-child(3) label {
	left: 70px;
}

.slider li:nth-child(4) label {
	left: 100px;
}

